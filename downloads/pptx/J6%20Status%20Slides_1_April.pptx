<script type="text/javascript">

  var acctValidationHandlerUrl = "/apan/ValidateUserAD.ashx";
  var trackLogInUrl = "/apan/TrackLogIn.ashx";
  var errorMessage = "";
  var successMessage = "";
  var group = "0";
  var mTxt = "Sign In<span class=\"hidden-phone\"> To Your Account</span>";
  var gTxt = "Enter room";
  var allowGuest = "False";

  $(document).ready(function () {

    if (errorMessage != "")
      $("#DivError-alert").show();
    else if (successMessage != "")
      $("#DivSuccess-alert").show();

    // set the focus on the first field
    $("#txtUserName").focus();


    $("#txtUserName").blur(function () {
      $("#divUserName").removeClass("success").removeClass("error");
      $("#UserNameMsg").text("");
    });

    // Client side onclick get focus
    $("#btnSubmit").click(function (event) {
    
      if ((allowGuest.toLowerCase() == "false" || $('#rdbtnMember').prop('checked')) && !validateUserName($("#txtUserName").val())) {
        //e.cancelBubble is supported by IE - this will kill the bubbling process.
        event.cancelBubble = true;
        event.returnValue = false;

        //e.stopPropagation works only in Firefox.
        if (event.stopPropagation) {
          event.stopPropagation();
          event.preventDefault();
        }
      }
    });

    // user clicks the cancel button, redirect to community 
    $("#btnCancel").click(function (event) {
      window.location.href = "https://www.apan.org";
    });

    function validateUserName(val) {

      var rtrnVal = false,
        msgAD = "";


      if (val && val != "")
      {        
        $.ajax({
          url: acctValidationHandlerUrl,
          cache: false,
          data: { Text: val },
          success: function (result) {
            switch (result) {
              case '5':
                rtrnVal = true;
                break;
              case '2':
                msgAD = "This account is disabled";
                trackLogin(result);
                break;
              case '3':
                msgAD = "This account is locked,  please try again in 15 minutes";
                trackLogin(result);
                break;
              case '4':
                trackLogin(result);
                rtrnVal = true;
                break;
              default:
                // (int)Common.ADAcctStatus.NotFound
                msgAD = "The username entered does not exist";
                trackLogin(result);
                break;                
            }; 
          },
          async: false
        });
      } else {        
        msgAD = "Enter a username.";
      }


      $("#UserNameMsg").text(msgAD);
      if (!rtrnVal) 
        $("#divUserName").removeClass("success").addClass("error");
      else 
        $("#divUserName").removeClass("error").addClass("success");

      return rtrnVal;
    }

    function trackLogin(val) {
        $.ajax({
          url: trackLogInUrl,
          cache: false,
          data: { username: $("#txtUserName").val(), status: val, translate: $("#txtTranslate").val() },
          async: true
        });
    }

    $('[name="guestAccess"]').change(function () {

    // Hide all
    $('#member,#guest').hide();
    $('.member,.guest').hide();

    // Show checked
    if ($('#rdbtnMember').prop('checked')) {
        $('#member').show();
        $(".member").show();
        $("#btnSubmit").text(mTxt);
    } else if ($('#rdbtnGuest').prop('checked')) {
        $('#guest').show();
        $(".guest").show();
        $("#btnSubmit").text(gTxt);
    } 
    });

  })



</script>
  
  
